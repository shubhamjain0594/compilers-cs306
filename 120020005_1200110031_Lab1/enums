        enum Tokens{VOID=0x100,
                    INT,
                    FLOAT,
                    IF,
                    ELSE,
                    FOR,
                    WHILE,
                    RETURN,
                    IDENTIFIER,
                    INC_OP,
                    EQ_OP,
                    LE_OP,
                    NE_OP,
                    GE_OP,
                    AND_OP,
                    OR_OP,
                    INT_CONSTANT,
                    STRING_CONSTANT,
                    FLOAT_CONSTANT,
                    WHITESPACES,
                    COMMENT,
                    OTHERS};
